#%echo Generating a rsa gpg key for encrypting and decrypting boxset zip file
# Key lengths have to be 1024 so the resulting base64 encoded string for the keys that is passed
# to cloudformation is not larger than 4096 bytes max size for a cloudformation parameter
Key-Type: RSA
Key-Length: 1024
Subkey-Type: RSA
Subkey-Length: 1024
Name-Real: ctgdevops-boxset-key
Name-Email: cgeodevopsengineering@intuit.com
Expire-Date: 0
Passphrase: ${gpg_passphrase}
#%echo Key generation complete

